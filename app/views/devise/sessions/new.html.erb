
<div class="col s9 m6  offset-s1  offset-m3">

   <h5 class="center-align " >Fazer Login</h5>
  <div class="card-panel minha-nav section ">




    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

    <div class="input-field">
      <i class="material-icons prefix">account_circle</i>
      <%= f.email_field :email, autofocus:true, class: "validate", id: "icon_prefix" %>

      <label for="icon_prefix">E-mail</label>
    </div>

    <div class="input-field">
      <i class="material-icons prefix">lock_outline</i>
      <%= f.password_field :password, autofocus:true, class: "validate", id: "icon_prefix" %>
      <label for="icon_prefix">Senha</label>
    </div>

    <% if devise_mapping.rememberable? -%>
    <div class="col s12 offset-s1">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
      <p></p>
    </div>
    <% end -%>

          <%= f.submit "Fazer Login", class: 'btn col s12' %>



  <div class="divider "></div>

<p></p>
<p class="magin">
    <%= render "devise/shared/links" %>
</p>



    <% end %>


  </div>
</div>
